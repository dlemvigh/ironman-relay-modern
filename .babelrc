{
    "presets": ["react-app"],
    "plugins": [
      "relay",
      ["transform-imports", {
        "react-router": {
          "transform": "react-router/es/${member}",
          "preventFullImport": true
        },
        "react-bootstrap": {
            "transform": "react-bootstrap/lib/${member}",
            "preventFullImport": true
        },
        "reactstrap": {
          "transform": "reactstrap/lib/${member}",
          "preventFullImport": true
        },
        "lodash": {
          "transform": "lodash/${member}",
          "preventFullImport": true
        }
    }]
    ]
  }